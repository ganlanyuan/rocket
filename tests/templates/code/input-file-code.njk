@import 'demo-base';
@import "../../src/scss/components/input-file"; 

.container { text-align: center; }
{% macro get(componentName) %}
{% if componentName === "xxx" %}

{% elif componentName === "input-file" %}
.input-file {
  @include input-file() {
    padding: 0.8em;
    font-size: 18px;
    color: #2E92E3;
    figure {
      display: table-cell;
      width: 120px;
      height: 120px;
      margin: 0;
      border-radius: 50%;
      background-color: #2E92E3;
      text-align: center;
      vertical-align: middle;
      path { fill: #fff; }
    }
    span { display: block; }
  }
}

{% endif %}
{% endmacro %}