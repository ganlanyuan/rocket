      "grid": {
        "p-1": [
          "Create a responsive multi columns (and even multi rows) layout using `-grid-`."
        ],
        "capability": "IE 8+, Firefox 28+, Chrome 21+, Safari 6.2+, Opera 15+",
        "syntax": true,
        "demo-1": {
          "markup": true,
          "scss": true
